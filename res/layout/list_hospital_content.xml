<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/list_padding_width"
    android:paddingRight="@dimen/list_padding_width" >

    <TextView
        android:id="@+id/cache_list_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/cache_list_title"
        android:layout_alignBottom="@+id/cache_list_title"
        android:layout_toRightOf="@+id/cache_list_title"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/cache_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/cache_list_address"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/cache_list_address"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/cache_list_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/cache_list_grade"
        android:layout_toRightOf="@+id/cache_list_image"
        android:layout_marginTop="8dp"
        android:layout_marginRight="@dimen/margin_right"
        android:layout_marginLeft="5dp"
        android:singleLine="true"
		android:ellipsize="end"
        android:text="TextView"
        android:textSize="@dimen/list_big_textsize" />

    <TextView
        android:id="@+id/cache_list_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/cache_list_title"
        android:layout_below="@+id/cache_list_title"
        android:layout_marginRight="@dimen/margin_right"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="TextView"
        android:textSize="@dimen/list_small_textsize" />

    <ImageView
        android:id="@+id/cache_list_moreinfo"
        android:layout_width="40dp"
        android:layout_height="70dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_margin="@dimen/list_detailimg_margin"
        android:clickable="true"
        android:focusable="false"
        android:src="@drawable/more_info" />

    <TextView
        android:id="@+id/cache_list_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cache_list_title"
        android:layout_toRightOf="@+id/cache_list_image"
        android:text="TextView"
        android:visibility="gone"
         />

    <TextView
        android:id="@+id/cache_list_grade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cache_list_id"
        android:layout_toLeftOf="@+id/cache_list_moreinfo"
        android:layout_toRightOf="@+id/cache_list_level"
        android:text="TextView"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/cache_list_image"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="1dp"
        android:scaleType="fitXY"
        android:src="@drawable/booking_hosp" />

</RelativeLayout>